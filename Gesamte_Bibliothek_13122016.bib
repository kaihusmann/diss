@Article{bonyad_evaluation_2012,
  Title                    = {Evaluation of non-destructive {Meyer} method for determination of bark volume of beech ({Fagus} orientalis {Lipsky}) in different geographical aspects},
  Author                   = {Bonyad, A. E. and Sima, A. and Bakhshandeh, A. and H., Dadras},
  Journal                  = {Caspian Journal of Environmental Sciences},
